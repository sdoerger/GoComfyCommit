{
  "profiles": [
    {
      "alias": "land",
      "commitMessage": "${t}: [${b}] ${m}",
      "cropBranchFromTo": [0, 8],
      "defaultCommitType": "fix"
    },
    {
      "alias": "exp",
      "commitMessage": "My CommitType: ${t} at Branch: ${b} > My Message: ${m}",
      "cropBranchFromTo": [0, 3],
      "defaultCommitType": "feat"
    }
  ]
}
