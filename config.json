{
  "profiles": [
    {
      "alias": "land",
      "commitMessage": "${t}: [${b}] ${m}",
      "cropBranchFromTo": [0, 8],
      "defaultCommitType": "docs"
    },
    {
      "alias": "exp",
      "commitMessage": "${t}/${b}: ${m}",
      "cropBranchFromTo": [0, 6],
      "defaultCommitType": "refactor"
    }
  ]
}
